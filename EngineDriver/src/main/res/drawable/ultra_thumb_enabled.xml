<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="@color/transparent"/>
            <stroke
                android:width="@dimen/buttonFnStroke_outline"
                android:color="@color/holoBlue"
                />
            <size
                android:width="20dp"
                android:height="40dp" />
            <corners android:radius="4dp" />

        </shape>
    </item>

    <item
        android:top="@dimen/buttonFnSelectedInsideStrokePadding_outline"
        android:left="@dimen/buttonFnSelectedInsideStrokePadding_outline"
        android:right="@dimen/buttonFnSelectedInsideStrokePadding_outline"
        android:bottom="@dimen/buttonFnSelectedInsideStrokePadding_outline">

        <shape
                android:shape="rectangle">
                <solid android:color="@color/buttonCommonFillGradiantCenter_ultra" />
            <stroke
                android:width="@dimen/buttonFnStroke_outline"
                android:color="@color/buttonCommonFillGradiantCenter_ultra"
                />
            <size
                    android:width="10dp"
                    android:height="20dp" />
                <corners android:radius="2dp" />

        </shape>
    </item>

    <item >
        <rotate android:fromDegrees="90">
        <shape
            android:shape="line">
            <stroke
                android:rotation="90"
                android:width="@dimen/buttonFnStroke_outline"
                android:color="@color/holoBlue"
                />
        </shape>
        </rotate>
    </item>


</layer-list>