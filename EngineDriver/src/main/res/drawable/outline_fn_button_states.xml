<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:state_enabled="false">
        <shape android:shape="rectangle"  >
            <corners android:radius="@dimen/buttonFnCornerRadius_outline" />
            <stroke android:width="@dimen/buttonFnDisabledStroke_outline" android:color="@color/buttonFnDisabledStroke_outline" />
            <solid android:color="@color/buttonFnDisabledFill_outline" />
        </shape>
    </item>

    <item android:state_pressed="true" android:state_enabled="true"  android:drawable="@drawable/outline_button_fn_layers_selected">
    </item>

    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="@dimen/buttonFnCornerRadius_outline" />
            <stroke android:width="@dimen/buttonFnStroke_outline" android:color="@color/buttonFnStroke_outline" />
            <solid android:color="@color/buttonFnFill_outline" />
        </shape>
    </item>
</selector>