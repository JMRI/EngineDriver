            android:layout_height="wrap_content"
            android:orientation="vertical">

            <Button
                android:id="@+id/limit_speed_0"
                style="?attr/ed_normal_button_style"
                android:layout_width="fill_parent"
                android:layout_marginEnd="7dp"
                android:layout_marginLeft="11dp"
                android:layout_marginRight="7dp"
                android:layout_marginStart="11dp"
                android:text="@string/limitSpeedButtonLabel" />

            <Button
                android:id="@+id/pause_speed_0"
                style="?attr/ed_normal_button_style"
                android:layout_width="fill_parent"
                android:layout_marginEnd="7dp" android:layout_marginLeft="11dp"
                android:layout_marginRight="7dp"
                android:layout_marginStart="11dp" android:text="@string/pauseSpeedButtonLabel" />

