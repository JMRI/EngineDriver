                        android:id="@+id/device_sounds_horn_short_0"
                        style="?attr/ed_fn_button_style"
                        android:layout_width="0dp"
                        android:layout_weight="5"
                        android:drawableLeft="@drawable/in_device_sounds_button_icon"
