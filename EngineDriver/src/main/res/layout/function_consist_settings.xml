            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/function_consist_heading"
                style="?android:attr/listSeparatorTextViewStyle"
                android:fontFamily="sans-serif-condensed"
                android:text="@string/functionConsistHeader"
                tools:ignore="RtlSymmetry" />

            <ScrollView
                android:id="@+id/function_consist_instruction_text_scroll_view"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/function_consist_heading"
                android:padding="0dp"
                tools:ignore="RtlSymmetry" >

                <TextView
                    android:id="@+id/function_consist_instruction_text"
                    style="?attr/floating_text_style"
                    android:fontFamily="sans-serif-condensed"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="2sp"
                    android:text="@string/functionConsistHelpText"
                    android:textSize="14sp"
                    tools:ignore="SmallSp" />

            </ScrollView>

            <Button
                android:id="@+id/functionConsistReset"
                style="?attr/ed_small_button_style"
                android:fontFamily="sans-serif-condensed"
                android:textSize="12sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@id/functionConsistClose"
                android:layout_centerHorizontal="true"
                android:text="@string/fbResetFunctionLabels"
                android:drawableLeft="?attr/ed_reset_glyph"
                android:paddingLeft="4dp"
                android:paddingStart="4dp"
                tools:ignore="RtlSymmetry" />

            <Button
                android:id="@+id/functionConsistClose"
                style="?attr/ed_small_button_style"
                android:fontFamily="sans-serif-condensed"
                android:textSize="12sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="40dp"
                android:layout_marginBottom="4dp"
                android:paddingLeft="4dp"
                android:paddingStart="4dp"
                android:paddingRight="4dp"
                android:paddingEnd="4dp"
                android:text="@string/logviewerClose"
                tools:ignore="RtlHardcoded" />

        </RelativeLayout>

